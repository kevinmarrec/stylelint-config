{
  "name": "@kevinmarrec/stylelint-config",
  "type": "module",
  "version": "1.2.2",
  "packageManager": "bun@1.2.22",
  "description": "Opinionated Stylelint config.",
  "author": "Kevin Marrec <kevin@marrec.io>",
  "license": "MIT",
  "homepage": "https://github.com/kevinmarrec/stylelint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinmarrec/stylelint-config.git"
  },
  "keywords": [
    "stylelint",
    "stylelint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "bun run check:unused && bun run check:eslint && bun run check:types",
    "check:eslint": "eslint . --cache",
    "check:types": "tsc --noEmit",
    "check:unused": "knip -n",
    "lint": "bun run check:eslint",
    "lint:inspect": "bunx @eslint/config-inspector",
    "release": "bumpp",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "stylelint": ">=16.18.0"
  },
  "dependencies": {
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-order": "^7.0.0",
    "stylelint-plugin-use-baseline": "^1.0.3"
  },
  "devDependencies": {
    "@kevinmarrec/eslint-config": "^1.5.3",
    "@kevinmarrec/tsconfig": "^1.1.0",
    "@types/bun": "^1.3.1",
    "@vitest/coverage-v8": "^4.0.6",
    "bumpp": "^10.3.1",
    "eslint": "^9.38.0",
    "knip": "^5.66.3",
    "taze": "^19.8.1",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  }
}
